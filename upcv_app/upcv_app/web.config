<configuration>
    <system.webServer>
        <handlers>
            <!-- Configuraci칩n para FastCGI -->
            <add name="DjangoFastCGI" path="*.py" verb="*" modules="FastCgiModule"
                 scriptProcessor="C:\Users\Administrador\Documents\Appscompras\upcv_app\venv\Scripts\python.exe|C:\Users\Administrador\Documents\Appscompras\upcv_app\venv\Scripts\wfastcgi.py"
                 resourceType="Unspecified" />
        </handlers>

        <staticContent>
            <!-- Configuraci칩n de archivos est치ticos -->
            <add fileExtension=".css" mimeType="text/css" />
            <add fileExtension=".js" mimeType="application/javascript" />
            <add fileExtension=".jpg" mimeType="image/jpeg" />
            <add fileExtension=".png" mimeType="image/png" />
            <add fileExtension=".woff" mimeType="application/font-woff" />
            <add fileExtension=".woff2" mimeType="application/font-woff2" />
            <add fileExtension=".ttf" mimeType="application/octet-stream" />
        </staticContent>

        <rewrite>
            <rules>
                <!-- Redirigir las solicitudes de archivos est치ticos -->
                <rule name="Static Files" stopProcessing="true">
                    <match url="^static/(.*)$" />
                    <action type="Rewrite" url="C:/Users/Administrador/Documents/Appscompras/upcv_app/staticfiles/{R:1}" />
                </rule>

                <!-- Redirigir las solicitudes de archivos de medios -->
                <rule name="Media Files" stopProcessing="true">
                    <match url="^media/(.*)$" />
                    <action type="Rewrite" url="C:/Users/Administrador/Documents/Appscompras/upcv_app/media/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
